{
  "name": "sharelist",
  "version": "0.3.11",
  "bin": "app.js",
  "repository": "https://github.com/reruin/sharelist",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "dev": "cross-env NODE_ENV=dev nodemon app.js -i ./cache",
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "pkg": "pkg . --output build/sharelist --targets linux-x64,linux-arm64,macos-x64,win-x64",
    "pkg-local": "pkg . --output build/sharelist",
    "release": "node ../../scripts/release.js --skipBuild --skipNpmPublish"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@sharelist/core": "^0.1",
    "@sharelist/webdav": "^0.1",
    "bonjour": "^3.5.0",
    "etag": "^1.8.1",
    "global": "^4.4.0",
    "ignore": "^5.1.8",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^10.0.0",
    "koa-sendfile": "^3.0.0",
    "koa-session-minimal": "^4.0.0",
    "koa-static-cache": "^5.1.4",
    "markdown-it": "^12.0.6",
    "mime": "^2.5.2",
    "nanoid": "^3.1.23",
    "node-fetch": "^2.6.1",
    "node-rsa": "^1.1.1",
    "webdav": "^4.6.0",
    "write-file-atomic": "^3.0.3",
    "xml2js": "^0.4.23",
    "yaml": "^1.10.2"
  },
  "pkg": {
    "scripts": [
      "./app/**/*",
      "./plugins/*",
      "./package/**/*"
    ],
    "assets": [
      "./theme/**/*"
    ]
  }
}
